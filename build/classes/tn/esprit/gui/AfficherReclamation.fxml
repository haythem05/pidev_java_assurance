                        <ListView fx:id="lvReclamation" layoutY="132.0" onMouseClicked="#functionTest" prefHeight="484.0" prefWidth="1285.0" />
                        <Button fx:id="refresh" layoutX="412.0" layoutY="93.0" mnemonicParsing="false" onAction="#refresh" style="-fx-background-color: #FFFFFF;" text="Refresh" />
                        <ImageView fx:id="image" fitHeight="261.0" fitWidth="302.0" layoutX="1318.0" layoutY="192.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="btnChatbot" layoutX="174.0" layoutY="93.0" mnemonicParsing="false" onAction="#openChatbotWindow" style="-fx-background-color: #FFFFFF;" text="Chatbot" />
                     </children>
                  </Pane>
               </children>
            </StackPane>
         </children>
      </AnchorPane>
   </children>
